{
  "name": "my-qwik-basic-starter",
  "description": "Demo app with sample routes",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/fastify/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "drizzle:check": "drizzle-kit check",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:pull": "drizzle-kit pull",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:up": "drizzle-kit up",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "postinstall": "drizzle-kit generate",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.fastify",
    "start": "vite --open --mode ssr --host",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.14.1",
    "@builder.io/qwik-city": "^1.14.1",
    "@eslint/js": "latest",
    "@qwik-ui/headless": "^0.6.7",
    "@qwik-ui/styled": "^0.3.1",
    "@qwik-ui/utils": "^0.3.2",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/vite": "^4.1.8",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "20.14.11",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.8.1",
    "class-variance-authority": "^0.7.0",
    "drizzle-kit": "^0.30.3",
    "drizzle-orm": "^0.39.0",
    "eslint": "9.25.1",
    "eslint-plugin-qwik": "^1.14.1",
    "globals": "16.0.0",
    "postcss": "^8.5.4",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "qwik-ui": "^0.3.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.4.5",
    "typescript-eslint": "8.26.1",
    "undici": "*",
    "vite": "5.3.5",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@fastify/compress": "^6.2.1",
    "@fastify/static": "^6.10.1",
    "fastify": "^4.17.0",
    "fastify-plugin": "^4.5.0"
  },
  "nx": {}
}
